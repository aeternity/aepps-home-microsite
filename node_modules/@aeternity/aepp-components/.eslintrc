{
  "root": true,
  "parser": "vue-eslint-parser",
  "parserOptions": {
    "parser": "babel-eslint",
    "sourceType": "module"
  },
  "globals": {
    "module": true
  },
  "env": {
    "node": true,
    "browser": true,
    "jest": true
  },
  "extends": [
    "airbnb-base",
    "plugin:vue/essential"
  ],
  "plugins": [
    "vue"
  ]
}
