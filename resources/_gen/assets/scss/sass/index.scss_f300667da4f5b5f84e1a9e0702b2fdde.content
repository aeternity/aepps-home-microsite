@import url("https://rsms.me/inter/inter-ui.css");
html, body {
  font-size: 16px;
  font-weight: normal;
  line-height: 1.75rem;
  box-sizing: border-box;
  caret-color: #FF0D6A;
  color: #1E1E1E;
  height: 100%;
  min-height: 100%;
  /* Better Font Rendering */
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale; }

* {
  outline: none;
  outline: 0;
  box-sizing: inherit; }
  *:hover, *:focus, *:active {
    outline: none;
    outline: 0; }
  *:before, *:after {
    box-sizing: inherit; }

/* GENERAL STYLES */
body {
  font-family: 'Inter UI', sans-serif;
  font-weight: 500;
  min-width: 320px;
  margin: 0px;
  padding: 0px; }

* {
  box-sizing: border-box;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box; }

h1 {
