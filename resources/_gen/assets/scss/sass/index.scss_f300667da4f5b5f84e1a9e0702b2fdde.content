    @media (max-width: 768px) {
      .title-subcategory {
        font-size: 1.5rem; } }

.app-icon {
  width: 64px;
  height: 64px;
  border-radius: 12px;
  -webkit-border-radius: 12px;
  -moz-border-radius: 12px;
  background-color: #ffffff;
  box-shadow: 0 0 6px 0 rgba(0, 0, 0, 0.2);
  -webkit-box-shadow: 0 0 6px 0 rgba(0, 0, 0, 0.2);
  -moz-box-shadow: 0 0 6px 0 rgba(0, 0, 0, 0.2);
  position: relative;
  display: inline-block;
  background-color: #ffffff; }

.app-icon img {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  -webkit-transform: translate(-50%, -50%);
  -moz-transform: translate(-50%, -50%);
  -ms-transform: translate(-50%, -50%); }

.btn {
  background-color: #F7296E;
  border-radius: 50px;
  -webkit-border-radius: 50px;
  -moz-border-radius: 50px;
  outline: none;
  color: #ffffff;
  padding: 12px 28px;
  cursor: pointer;
  font-weight: 500;
  line-height: 1.44;
  top: 0px;
  left: 0px;
  position: relative;
  transition: all 0.2s ease-in-out;
  -webkit-transition: all 0.2s ease-in-out;
  -moz-transition: all 0.2s ease-in-out;
  -o-transition: all 0.2s ease-in-out; }

.btn:hover, .btn:active, .btn:focus {
  top: -2px;
  left: -1px;
  color: #ffffff;
  box-shadow: 0 4px 8px 0 rgba(60, 60, 60, 0.1);
  -webkit-box-shadow: 0 4px 8px 0 rgba(60, 60, 60, 0.1);
  -moz-box-shadow: 0 4px 8px 0 rgba(60, 60, 60, 0.1); }

.btn.btn-secondary {
  background-color: #311B58; }

.btn.btn-third {
  background-color: #989898; }

.btn.btn-small {
  padding: 6px 16px;
  text-transform: uppercase;
  font-size: 14px; }

.tag {
  font-size: 12px;
  border-radius: 5px;
  -webkit-border-radius: 5px;
  -moz-border-radius: 5px;
  padding: 4px 13px;
  color: #ffffff;
  display: inline-block;
  background-color: #f7296e;
  text-transform: uppercase; }

.tag.tag-version {
  background-color: #311b58; }

.tag.core-app {
  background-color: #989898; }

.cards {
  margin-left: -15px;
  margin-right: -15px; }

.cards .card-col {
  width: 33.3%;
  float: left;
  margin-bottom: 33px; }

.cards .card {
  float: left;
  position: relative;
  padding: 20px;
  margin: 0px 15px;
  min-height: 335px;
  text-align: left;
  border-radius: 14px;
  -webkitborder-radius: 14px;
  -moz-border-radius: 14px;
  background-color: #f4f4f4;
  transition: all 0.3s ease-in-out;
  -webkit-transition: all 0.3s ease-in-out;
  -moz-transition: all 0.3s ease-in-out;
  -o-transition: all 0.3s ease-in-out;
  top: 0px;
  left: 0px;
  cursor: pointer; }

.cards .card:hover {
  top: -4px;
  left: -2px;
  box-shadow: 0 7px 10px 0 rgba(0, 0, 0, 0.15);
  -webkit-box-shadow: 0 7px 10px 0 rgba(0, 0, 0, 0.15);
  -moz-box-shadow: 0 7px 10px 0 rgba(0, 0, 0, 0.15); }

.cards .card img + span {
  display: block;
  margin: 30px 0px 10px 0px;
  font-size: 14px;
  font-weight: 500;
  color: #f7296e; }

.cards .card strong {
  display: block;
  font-size: 24px;
  font-weight: bold;
  line-height: 1.25;
  text-align: left;
  color: #000000;
  margin-top: 10px;
  margin-bottom: 10px;
  height: 150px;
  overflow: hidden; }

.cards .card img {
  position: absolute;
  top: 12px;
  right: 16px;
  width: 44px; }

.cards .card .right {
  display: block;
  margin: 0px; }

.cards .card .right .btn {
  display: inline-block; }

.cards .card .tag {
  margin: 0px 0px 2px 0px; }

.cards .card span.tag-wrapper {
  position: absolute;
  bottom: 20px;
  left: 20px;
  right: 20px; }

/* desktop only */
@media only screen and (min-width: 1024px) {
  .cards .card-col.clearfix-lg {
    clear: both; } }

/* tablet*/
@media only screen and (max-width: 1023px) and (min-width: 640px) {
  .cards .card-col {
    width: 50%;
    margin-bottom: 20px; }
  .cards .card {
    max-width: 280px; }
  .cards .card-col.clearfix-md {
    clear: both; } }

/* mobile */
@media only screen and (max-width: 639px) {
  .cards .card-col {
    float: none;
    width: 100%;
    margin-bottom: 20px;
    text-align: center; }
  .cards .card {
    float: none;
    display: inline-block;
    max-width: 280px; } }

.filter {
  position: absolute;
  border-radius: 10px;
  -webkit-border-radius: 10px;
  -moz-border-radius: 10px;
  z-index: 10;
  background-color: #ffffff;
  width: 335px;
  right: 0px;
  top: 22px;
  font-size: 14px;
  text-align: right;
  color: #000000;
  overflow: hidden;
  cursor: pointer; }

.filter.active {
  box-shadow: 0 2px 8px 0 rgba(60, 60, 60, 0.1);
  -webkit-box-shadow: 0 2px 8px 0 rgba(60, 60, 60, 0.1);
  -moz-box-shadow: 0 2px 8px 0 rgba(60, 60, 60, 0.1); }

.filter span.current {
  position: relative;
  padding: 17px 0px;
  display: block;
  font-size: 18px;
  font-weight: 500;
  color: #1e1e1e;
  margin-right: 30px; }

.filter span.current::after {
  content: " ";
  position: absolute;
  width: 20px;
  height: 20px;
  background-image: url("../icons/chevron-down.svg");
  background-size: 20px;
  background-position: center;
  background-repeat: no-repeat;
  top: 50%;
  margin-top: -10px;
  right: -30px; }

.filter.active span.current {
  display: none; }

.filter ul {
  display: none;
  list-style: none;
  padding: 0px;
  text-align: center; }

.filter.active ul {
  display: block;
  margin: 0px; }

.filter ul li {
  padding: 17px 0px; }

.filter ul li:hover {
  background-color: #f5f5f5; }

